@model CumulativeProject3.Models.Course

@{
    ViewBag.Title = "DeleteConfirm";
}

<div id="containershow">
    <nav>
        <a class="deletelink" href="/Course/Show/@Model.CourseId">Cancel</a>
    </nav>
    <h2 class="title">Are you sure you want to delete @Model.CourseName</h2>
    <p>This action cannot be undone.</p>
    <form method="POST" action="/Course/Delete/@Model.CourseId">
        <button class="delbutton">
            Confirm Delete
        </button>
    </form>
</div>





